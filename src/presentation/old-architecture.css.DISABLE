	#navBar .popup1 span.button.trimmed i { width:50px; margin:0.5em 0.3em; } 
    div.fullWidth { width:calc( 100vw - 1.5em ); }
	div.fullWidth h1 { padding-top:0.1em; padding-left:0em; }	
	#shareGroup { margin-left:0em; padding-left:0em; }
	.tabList.tabs li { margin-left:0; text-align:center; }

	a sup i.fa.invert { font-size:90%; }
