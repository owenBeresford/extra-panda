/* https://stackoverflow.com/questions/12251750/can-media-queries-resize-based-on-a-div-element-instead-of-the-screen 
@element '.adjacentItem p'
*/

/* this would look better if I had the ability to adjust only sup that are next to sup, rather than text; but I do not have this option.

1. descendant selector (space) : .outer h2: All descendants i
2. child selector (>) : .outer  margin-left:0px; > h2 : Immediate Child 
3. adjacent sibling selector (+) : .outer + h2 : Immediate Sibling from same parent
4. general sibling selector (~) : .outer ~ h2 : All Sibling from same parent â€“ 
*/
@media screen and (min-resolution: 150dpi) {
	.h4_page p, .h4_page li, .h4_page span, .h4_page a, label { font-size:120%; width:fit-content; max-width:100vw; }
	h2.biblioSection { font-size:150%; }

    span.button.smallScreenOnly { font-size:120%; padding:0.5em 0.3em; }
	i.fa, div#navBar a, .h4_page .tabs-content p, .h4_page .tabs-content li, .h4_page .tabs-content a { font-size:120%; }
 
	.h4_page.wholeArticle { width:fit-content; }
	.h4_page li li, .h4_page li span, .h4_page li a, div.h4_page p span, div.h4_page p sup a, div.h4_page p a, div.h4_page li a { font-size:100%; width:fit-content;  max-width:100vw; }
	li { margin:2px 10px; }
	h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5 { font-size:130%; width:fit-content; }
	h1, .h1 { font-size: 3.5rem; }
	.h4_page .tabs-content li a { font-size:100%; }
	.tabList li a, .tabList li { padding-left:0px; padding-right:0px; }
	.column { padding-left:0px; padding-right:0px; }
	ul.tabList.tabs, fieldset.tabs-content { width:100%; max-width:100vw; }
   .adjacentGroup { min-width:99vw; }
	.adjacentList { height:auto; display:block;  width:98vw; }
    span.adjacentItem, .adjacentItem { display:block; margin-left:0.5em; height:auto; margin-bottom:0.5em; width:calc( 100vw - 3em ); }
	.adjacentItem .button { width:96vw; text-align:center; max-width:96vw; min-width:96vw; }

    #navBar { display:block; width:100vw; }
    #navBar.row { max-width:min-content; }
	.outer_menu { width:calc( 100vw - 0.5em ); /* flex:1 1 100vw; */ }
	#burgerMenu { display:none; }
	.halferWords { width:100vw; align-self: stretch; }
	.wholeArticle { align-items: stretch; }
	#navBar a i { margin:1em 0.7em; }
	#navBar a svg { margin-left:0.4em; }
	.after_menu.articleContent { min-width:90%; }
	footer { width:100%; }
	ol.Alist li { font-size:inherit; }
	ol.Alist cite { font-size: 120%; }
	ol.Alist { width:80%; margin-left:auto; margin-right:auto; }
    a.button, del.button { margin-left:1em; }
	#shareGroup a.button:first-child { margin-left:0em; width:5em; }
	.adjacentItem a.button { margin-left:0em; } 
	sub, sup { font-size: 130%; }
	.lotsOfWords { column-gap:0em; column-count:initial; columns: initial; }
	sup a { line-height:1.5; }
    #navBar .shareMenuOpen a i { margin: .3em; height: 1.5em; display: inline-block; }
    #navBar .shareMenuOpen a i.fa-copy { margin: 3px 0px; padding: 0px; position: relative; left: 0.1em; }
    #navBar .shareMenuOpen a.button  { margin-left:0.40em; }
	.popup1 .fa-cancel { margin:0px; }
	#navBar .popup1 a.button { margin-left: 0px; width:100vw; }
	#navBar .popup1 a#hideMasto1 { padding: 0.45em 0.75em; }
	#shareMenuTrigger { margin-left:0px; }
	div#navBar .shareMenuOpen a { font-size:120%; }
	.shareMenuOpen a i.fa { font-size:150%; }
	.adjacentItem:hover p { display:none; }
	input[type='text'], #fSearch .button { font-size:130%; }
	i.specialPointer { font-size:6em; left: -0.3em; top: 0.25em; }

    #rssLink { width: 2.3em; }
	#navBar a#rssLink i { margin:inherit; }
/*	div#navBar .shareMenuOpen li:nth-child(3) a { position: relative; left:-1em; } */
}

