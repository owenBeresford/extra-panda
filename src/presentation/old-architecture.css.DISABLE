/* general resets */
ul, ol { list-style: none; }
.h4_page { direction:ltr; }

.ulbasic li::before, ol li::before { content: "âœ¨"; display: inline-block; width: 1.5em; }
a sup i.fa.invert { transform: rotate(90deg); font-size:70%; }
h2 sup { font-size:50%; }
.h4_footer { margin:20px; text-align:center; font-size: 70%; }
.h4_footer .leftFooter { display:inline-block; width:180px; height:50px; vertical-align:top; float:left; text-align:right;  }
.h4_footer .leftFooter img { vertical-align:top; }
.h4_footer p { margin-bottom:0.2em; }
